{
    "name": "grigio/notifications",
    "description": "Notification handler",
    "authors": [
        {
            "name": "Farshad Fakhar",
            "email": "farhsadfakhar@gmail.com"
        }
    ],
    "keywords": [
      "notification",
      "sms",
      "email",
      "smsir",
      "sms_ir"
  ],
    "require": {
      "php": ">=5.5",
      "ext-json": "*",
      "guzzlehttp/guzzle": "^6.5.2"
    },
    "autoload": {
        "psr-4": {
            "Grigio\\Notifications\\": "src/"
        }
      },
      "minimum-stability": "dev",
      "extra": {
        "laravel": {
          "providers": [
            "Grigio\\Notifications\\NotificationServiceProvider"
          ],
          "aliases": {
            "Notification": "Grigio\\Notifications\\NotificationFacade"
          }
        }
      }
}
